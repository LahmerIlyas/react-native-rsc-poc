{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "types": [
      "node"
    ],
    "moduleResolution": "node16",
    "module": "node16",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "declaration": true
  },
  "files": [],
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.js",
    "src/**/*.jsx",
    "../../types/**/*.d.ts",
    "src/types/*.d.ts",
    "./*.js"
  ]
}
